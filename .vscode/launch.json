{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Remote Attach",
      "type": "debugpy",
      "request": "attach",
      "preLaunchTask": "docker-run: debug",
      "connect": {
        "host": "localhost",
        "port": 5678
      },
      "pathMappings": [{
        "localRoot": "${workspaceFolder}",
        "remoteRoot": "/app"
      }
      ]


    }
  ]
}
